    "express": "^4.15.5",
    "jade": "~1.11.0",
    "mongoose": "^5.0.6",
    "morgan": "~1.9.0",
    "nodemon": "^1.15.0",
    "npm": "^5.7.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^1.0.0",
